{
  "containerDefinitions": [
    {
      "name": "app",
      "image": "733626105378.dkr.ecr.ap-south-1.amazonaws.com/charging-stations-simulator:latest",
      "cpu": "512",
      "memory": "512",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "DD_ENV",
          "value": "prod"
        },
        {
          "name": "DD_SERVICE",
          "value": "charger-simulator"
        },
        {
          "name": "DD_VERSION",
          "value": "1.0.0"
        }
      ],
      "dockerLabels": {
        "com.datadoghq.tags.env": "prod",
        "com.datadoghq.tags.service": "charger-simulator",
        "com.datadoghq.tags.version": "1.0.0"
      }
    }
  ],
  "family": "prod-td-simulator-service",
  "executionRoleArn": "arn:aws:iam::733626105378:role/prod-simulator-service-ecs-srv-role"
}
